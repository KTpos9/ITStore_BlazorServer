@using Web_Project.Authentication;

@inject AuthenticationStateProvider authStateProvider
@inject NavigationManager navManager

<nav class="navbar navbar-expand-sm navbar-dark py-3 fs-5 px-5 fixed-top" style="background-color: #12C480;">
  <div class="container-fluid">
    <a class="navbar-brand" href="">bITShop</a>
        <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
    <div class="collapse navbar-collapse justify-content-between" id="navbarSupportedContent">
      <ul class="navbar-nav">

        <!-- Cart -->
        <li class="nav-item">
          @*<a class="nav-link" href="#">Link</a>*@
          <AuthorizeView>
              <Authorized>
                  <NavLink class="nav-link" href="cart">Cart</NavLink>
              </Authorized>
              <NotAuthorized>
                  <NavLink class="nav-link disabled" href="cart">Cart</NavLink>
              </NotAuthorized>
          </AuthorizeView>
        </li>

        <!-- Drop Down -->
        <AuthorizeView Roles="Administrator">
            <Authorized>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                        Admin Pannel
                    </a>
                    <ul class="dropdown-menu" aria-labelledby="navbarDropdown">
                        <li><a class="dropdown-item" href="productedit">Edit Product</a></li>
                        <li><a class="dropdown-item" href="memberedit">Edit Menber</a></li>
                        <li><hr class="dropdown-divider"></li>
                        <li><a class="dropdown-item" href="#">Something else here</a></li>
                    </ul>
                </li>
            </Authorized>
        </AuthorizeView>
        

        <!-- Disabled -->
        <li class="nav-item">
          <NavLink class="nav-link" href="discountCode">Coupon</NavLink>
          @*<a class="nav-link disabled">Disabled</a>*@
        </li>
      </ul>

      <!-- Search -->
      <form class="d-flex flex-md-grow-0 flex-md-shrink-1 flex px-5" style="flex-basis:720px;">
        <input class="form-control me-2 mx-lg-auto" type="search" placeholder="Search" aria-label="Search">
        <button class="btn btn-outline-success" type="submit">Search</button>
      </form>

      <!-- Profile -->
      <div class="d-flex justify-content-end" style="min-width:225px;">
          <div class="d-flex justify-content-end align-items-center">
              <img class="rounded-circle d-block"style="width:35px;height:35px;background-color:azure" src="data:image/png;base64," alt=" ic_fluent_person_24_filled" />
              <div class="d-flex flex-column px-2 pt-2">
                  <AuthorizeView>
                      <Authorized>
                          <h5 style="font-size:15px;color:white;padding:0">@context.User.Identity.Name</h5>
                          <a href="" style="font-size:15px;color:white" @onclick="Logout">logout</a>
                      </Authorized>
                      <NotAuthorized>
                        <h5 style="font-size:15px;color:white;padding:0">Hello, Guest</h5>
                        <a href="login" style="font-size:15px;color:white">login</a>
                      </NotAuthorized>
                  </AuthorizeView>
              </div>
          </div>
      </div>
    </div>
  </div>
</nav>

@code {
    private async Task Logout()
    {
        var customAuthProvider = (CustomAuthProvider)authStateProvider;
        await customAuthProvider.UpdateAuthenticationStateAsync(null);
        navManager.NavigateTo("/", true);
    }
}
